{
  "presets": ["env", "stage-3", "react", "flow"],
  "plugins": [
    [
      "transform-runtime",
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".js", ".jsx"],
        "alias": {
          "containers": "./app/containers",
          "components": "./app/components",
          "routers": "./app/routers"
        }
      }
    ]
  ]
}
